
@{
    ViewBag.Title = "Dangkinhantin";
    Layout = "~/Views/Shared/_Layout.cshtml";
    IQueryable<Model.EF.dknt> dknt = ViewData["dknt"] as IQueryable<Model.EF.dknt>;
}

<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Danh sách email đăng kí nhận tin</h1>
            </div>
            <!-- /.col-lg-12 -->
            <div id="Hienthi">
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr align="center">
                            <th style="width: 30px">ID</th>
                            <th>Email</th>
                            <th>Ngày đăng kí</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var dkntm in dknt)
                        {
                            <tr class="odd gradeX" style="text-align: center">
                                <td>@dkntm.id</td>
                                <td>@dkntm.email</td>
                                <td>@dkntm.ngaydk</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>