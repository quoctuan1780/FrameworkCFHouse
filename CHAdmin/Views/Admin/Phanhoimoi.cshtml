
@{
    ViewBag.Title = "Phanhoi";
    Layout = "~/Views/Shared/_Layout.cshtml";
    IQueryable<Model.EF.phanhoi> phanhoimoi = (IQueryable<Model.EF.phanhoi>) ViewData["phanhoimoi"];
}

<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Thông tin phản hồi mới</h1
            </div>
            <!-- /.col-lg-12 -->
            <div>
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr align="center">
                            <th>Vấn đề</th>
                            <th>Email</th>
                            <th>Khách hàng</th>
                            <th>Ngày phản hồi</th>
                            <th>Xóa</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var phm in phanhoimoi){
                        <tr class="odd gradeX" style="text-align: center">
                            <td><a href="javascript:void(0)" id="@phm.maph" class="@phm.maph" onclick="showContent(document.getElementById(@phm.maph))">@phm.vande</a></td>
                            <td>@phm.email</td>
                            <td>@phm.hoten</td>
                            <td>@phm.ngayph</td>
                            <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="javascript:void(0)" onclick="ConfirmDelete(@phm.maph)">Xóa</a></td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div id="Hienthi"></div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>